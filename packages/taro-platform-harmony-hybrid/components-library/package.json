{
  "name": "@tarojs/components-harmony-hybrid",
  "version": "3.6.25",
  "description": "Taro 组件库 harmony-hybrid 版本库",
  "private": true,
  "browser": "dist/index.js",
  "main:h5": "dist/index.js",
  "main": "dist/index.js",
  "module": "dist/index.cjs.js",
  "types": "types/index.d.ts",
  "sideEffects": [
    "*.scss",
    "*.css"
  ],
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "scripts": {
    "prebuild": "run-p generate:*",
    "build": "cross-env NODE_ENV=production run-s build:components && pnpm build:ci",
    "build:components": "stencil build",
    "dev:components": "cross-env NODE_ENV=development pnpm run build:components --watch",
    "generate:stencil-config": "esbuild ./scripts/stencil/stencil.config.ts --external:lightningcss --bundle --platform=node --outfile=stencil.config.js",
    "build:ci": "run-s clean prod",
    "clean": "rimraf ./lib",
    "dev": "pnpm run prod -w",
    "preprod": "node ./scripts/fix.js",
    "prod": "rollup -c --bundleConfigAsCjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NervJS/taro.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@stencil/core": "^2.22.2",
    "@tarojs/taro": "workspace:*",
    "react-dom": "^18.2.0",
    "tslib": "^2.6.2",
    "vue-fragment": "^1.6.0",
    "classnames": "^2.2.5",
    "hls.js": "^1.1.5"
  },
  "devDependencies": {
    "@babel/generator": "^7.20.0",
    "@babel/parser": "^7.20.0",
    "@babel/traverse": "^7.20.0",
    "@babel/types": "^7.20.0",
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.5",
    "@stencil/react-output-target": "0.4.0",
    "@stencil/vue-output-target": "0.7.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@types/node": "^14.14.31",
    "change-case": "^4.1.2",
    "csstype": "^3.1.1",
    "esbuild": "~0.19.5",
    "lightningcss": "^1.22.1",
    "lodash": "^4.17.21",
    "miniapp-types": "1.6.0",
    "rollup": "^3.8.1",
    "babel-preset-taro": "workspace:*",
    "postcss": "^8.4.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup2": "npm:rollup@^2.79.0",
    "rollup-plugin-node-externals": "^5.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-ts": "^3.0.2",
    "typescript": "^4.7.4",
    "vue3": "npm:vue@^3.0.0",
    "sass": "^1.58.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.14.1"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-native": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  }
}
