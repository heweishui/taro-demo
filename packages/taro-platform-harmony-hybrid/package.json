{
  "name": "@tarojs/plugin-platform-harmony-hybrid",
  "version": "3.6.23",
  "description": "Harmony 端平台插件",
  "author": "ZakaryCode",
  "license": "MIT",
  "main": "index.js",
  "keywords": [
    "taro"
  ],
  "files": [
    "dist",
    "index.js",
    "types"
  ],
  "sideEffects": [
    "./dist/runtime/index.js",
    "*.scss",
    "*.css"
  ],
  "types": "types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NervJS/taro.git"
  },
  "scripts": {
    "prebuild": "run-s clean generate:desc",
    "build": "pnpm run rollup --environment NODE_ENV:production",
    "clean": "rimraf dist",
    "dev": "pnpm run rollup --environment NODE_ENV:development -w",
    "generate:desc": "pnpm run tsx --files ./build",
    "rollup": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript --bundleConfigAsCjs",
    "tsx": "ts-node --skipIgnore"
  },
  "bugs": {
    "url": "https://github.com/NervJS/taro/issues"
  },
  "dependencies": {
    "@tarojs/api": "workspace:*",
    "@tarojs/components": "workspace:*",
    "@tarojs/components-react": "workspace:*",
    "@tarojs/router": "workspace:*",
    "@tarojs/runtime": "workspace:*",
    "@tarojs/service": "workspace:*",
    "@tarojs/shared": "workspace:*",
    "@tarojs/taro-h5": "workspace:*",
    "babel-plugin-transform-taroapi": "workspace:*",
    "base64-js": "^1.3.0",
    "change-case": "^4.1.2",
    "classnames": "^2.2.5",
    "hls.js": "^1.1.5",
    "intersection-observer": "^0.12.2",
    "jsonp-retry": "^1.0.3",
    "lodash-es": "4.17.21",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "resolve": "^1.22.0",
    "vue": "^2.6.11",
    "weui": "^1.1.2",
    "whatwg-fetch": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.14.5",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-typescript": "^4.1.2",
    "@types/react": "^18.2.0",
    "@types/vue": "^2.0.0",
    "@vue/compiler-sfc": "^2.6.11",
    "fast-glob": "^3.3.1",
    "lodash": "^4.17.21",
    "rollup": "^3.8.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-node-externals": "^5.0.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-ts": "^3.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.7.4",
    "vue-loader": "^15.9.2",
    "vue-template-compiler": "^2.6.11"
  }
}
